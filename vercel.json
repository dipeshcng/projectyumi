{
    "version": 2,
    "builds": [
        {
        "src": "projectyumi/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
    },
    {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": { 
            "distDir" : "staticfilesh_build"
         }
    }
],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "projectyumi/wsgi.py"
        }
    ]
}